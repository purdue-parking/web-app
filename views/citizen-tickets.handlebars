
<div id="table-container">
	<table class="table table-hover">
		<thead>
			<tr>
				<th>Date</th>
				<th>Ticket Number</th>
				<th>License Plate</th>
				<th>Reason</th>
				<th>Actions</th>
			</tr>
		</thead>
		<tbody>
			{{#each items}}
				<tr>
					<td>{{{date}}}</td>
					<td class=".ticket-num">{{ticketNum}}</td>
					<td>{{plateNum}}</td>
					<td>{{{reason}}}</td>
					<td><button type="button" class="btn btn-primary pay-btn">Pay</button>
						<button type="button" class="btn btn-primary appeal-btn">Appeal</td>
				</tr>
			{{/each}}
		</tbody>
	</table>
</div>

<script src="/public/js/citizen-tickets.js"></script>